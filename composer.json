{
    "name": "vigihdev/faker-provider",
    "description": "A Faker provider for generating mock data",
    "homepage": "https://github.com/vigihdev/faker-provider",
    "keywords": [
        "faker-provider",
        "mockforge",
        "wordpress",
        "data",
        "mock"
    ],
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Vigih Dev",
            "email": "vigihdev@gmail.com",
            "role": "developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.thrubus.id"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/filesystem": "^6.4",
        "fakerphp/faker": "^1.23",
        "symfony/dotenv": "^6.4",
        "vigihdev/symfony-bridge-config": "dev-main",
        "fightbulc/moment": "^1.35",
        "vigihdev/serializer": "dev-main",
        "vigihdev/support": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Vigihdev\\Faker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigihdev\\Faker\\Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "dev": "php -S localhost:3312 local-dev/index.php"
    }
}
